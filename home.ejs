<%- include("partials/header"); -%>
<h1>Home</h1>
<p><%= homeText %></p>
<% for(let i=0;i< blogs.length;i++) { %>
    <h1><%= blogs[i].titlehead %></h1>

    <p><%= blogs[i].content.slice(0,100) %>
    <% if(blogs[i].content.length>100) { %>
        ...
        <a href="/blogposts/<%= blogs[i].titlehead %>" >[Read More]</a>
    <% } %>
    </p>
<% } %>
<%- include("partials/footer"); -%>